  },

  click = {
    arguments = {{meta = ""}},
    summary = "Represent a single \"click\" in a GUI.  Used within '@clicksequence'.",
    documentation = "info:texinfo#Click Sequences"
  },

  clicksequence = {
    arguments = {
      {meta = "ACTION"},
