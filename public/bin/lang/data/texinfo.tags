    documentation = "info:texinfo#Click Sequences"
  },

  clickstyle = {
    arguments = {
      {meta = "@CMD"}
    },     
    summary = "Execute @CMD for each '@click'; the default is '@arrow'.  The usual following empty braces on @CMD are omitted.",
